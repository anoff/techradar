{
  "name": "devradar",
  "private": true,
  "description": "Competence management for developers",
  "author": "Andreas Offenhaeuser <https://anoff.io>",
  "scripts": {
    "build": "vue-cli-service build",
    "cy:ci": "cypress run --record",
    "cy:ci:firebase": "cypress run --record --group firebase --ci-build-id ${GITHUB_SHA}",
    "cy:ci:localstorage": "cypress run --record --group localstorage --ci-build-id ${GITHUB_SHA}",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:electron": "cypress run --browser electron",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "serve:static": "serve dist --single --no-clipboard --listen 8080",
    "serve:test": "vue-cli-service serve --mode test",
    "start": "npm run serve",
    "test": "npm run lint && npm run build",
    "test:e2e:run": "run-p --race serve:static cy:run",
    "test:e2e:run:firefox": "run-p --race serve:static cy:run:firefox",
    "test:e2e:run:chrome": "run-p --race serve:static cy:run:chrome",
    "test:e2e:run:electron": "run-p --race serve:static cy:run:electron",
    "test:e2e:ci:firebase:record": "run-p --race serve:static cy:ci:firebase",
    "test:e2e:ci:localstorage:record": "run-p --race serve:static cy:ci:localstorage",
    "test:e2e:local": "npm run build -- --mode development && npm run test:e2e:run && npm run build -- --mode test && npm run test:e2e:run",
    "test:local": "npm run lint && npm run test:e2e:local"
  },
  "dependencies": {
    "@devradar/backend-firebase": "^1.0.0",
    "@iarna/toml": "^2.2.5",
    "@types/d3": "^5.7.2",
    "@types/uuid": "^8.0.0",
    "d3": "^5.16.0",
    "firebase": "^7.17.1",
    "firebaseui": "^4.6.0",
    "markdown-it": "^11.0.0",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-class-component": "^7.2.5",
    "vue-cookie-law": "^1.13.3",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-tour": "^1.5.0",
    "vuetify": "^2.3.5",
    "vuex": "^3.5.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.3.45",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "cypress": "^4.11.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-admin": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "serve": "^11.3.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "typescript": ">=3.2.1 <3.6.0",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "cz-conventional-kawaii": {
      "scopes": [
        "app"
      ],
      "quick": false
    }
  },
  "license": "MIT"
}
