{
  "name": "devradar",
  "private": true,
  "description": "Competence management for developers",
  "author": "Andreas Offenhaeuser <https://anoff.io>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --format unix",
    "start": "npm run serve",
    "start:ci": "serve dist --single --no-clipboard --listen 8080",
    "test": "npm run lint && npm run build",
    "cypress:ci": "cypress run --record",
    "test:e2e": "cypress open",
    "test:e2e:ci": "run-p --race start:ci cypress:ci"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "@types/d3": "^5.7.2",
    "@types/uuid": "^3.4.5",
    "axios": "^0.19.0",
    "clipboard-copy": "^3.0.0",
    "core-js": "^3.1.3",
    "d3": "^5.9.7",
    "firebase": "^6.1.0",
    "lz-string": "^1.4.4",
    "markdown-it": "^8.4.2",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.0",
    "vue-class-component": "^7.0.2",
    "vue-cookie-law": "^1.10.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.6",
    "vuetify": "^2.1.1",
    "vuetify-upload-button": "^2.0.0",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.4.95",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^3.6.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.23.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "cz-conventional-kawaii": {
      "scopes": [
        "app"
      ],
      "quick": false
    }
  },
  "license": "MIT"
}
